  "name": "Modus Operandi",
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6f6f6f"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#006800"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#a60000"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#531ab6"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#005e8b"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function.core"
      ],
      "settings": {
        "foreground": "#721045"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#005f5f"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable.ruby",
        "variable.other.readwrite.instance.ruby"
      ],
      "settings": {
        "foreground": "#005e8b"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#2f3f83"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#ffdfa9"
      }
    },
    {
      "scope": "meta.diff.removed",
      "settings": {
        "foreground": "#a60000"
      }
    },
    {
      "scope": "meta.diff.added",
      "settings": {
        "foreground": "#006800"
      }
    },
    {
      "scope": "meta.diff.changed",
      "settings": {
        "foreground": "#6f5500"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#005e8b"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#005e8b"
      }
    }
